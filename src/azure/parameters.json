{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "eastus"
        },
        "networkSecurityGroupRules": {
            "value": [
                {
                    "name": "SSH",
                    "properties": {
                        "priority": 300,
                        "protocol": "TCP",
                        "access": "Allow",
                        "direction": "Inbound",
                        "sourceAddressPrefix": "*",
                        "sourcePortRange": "*",
                        "destinationAddressPrefix": "*",
                        "destinationPortRange": "22"
                    }
                }
            ]
        },
        "jumpboxVirtualMachineName": {
            "value": "JumpBoxVM"
        },
        "db2VirtualMachineNamePrefix": {
            "value": "db2VM"
        },
        "mqVirtualMachineName": {
            "value": "mqVM"
        },
        "osDiskType": {
            "value": "Premium_LRS"
        },
        "virtualMachineSize": {
            "value": "Standard_B2ms"
        },
        "adminUsername": {
            "value": "azureuser"
        },
        "vnetName": {
            "value": "vnet1"
        },
        "vnetAddressPrefix": {
            "value": "10.0.0.0/16"
        },
        "subnetControlNodePrefix": {
            "value": "10.0.0.0/24"
        },
        "subnetControlNodeName": {
            "value": "control"
        },
        "subnetEndpointsPrefix": {
            "value": "10.0.3.0/24"
        },
        "subnetEndpointsName": {
            "value": "endpoints"
        },
        "subnetWorkerNodePrefix": {
            "value": "10.0.1.0/24"
        },
        "subnetWorkerNodeName": {
            "value": "worker"
        },
        "subnetBastionPrefix": {
            "value": "10.0.2.0/26"
        },
        "subnetBastionName": {
            "value": "AzureBastionSubnet"
        },
        "subnetVMName": {
            "value": "VMs"
        },
        "subnetVMPrefix": {
            "value": "10.0.5.0/26"
        },
        "storageNamePrefix": {
            "value": "sterlingfiles"
        },
        "bastionHostName": {
            "value": "sterlingBastionHost"
        },
        "mqsharename":{
            "value": "mqha"
        },
        "loadBalancerName":{
            "value": "db2-lb"
        },
        "gatewayName":{
            "value": "workerGateway"
        },
        "db2lbprivateIP":{
            "value": "10.0.4.100"
        },
        "devVMName":{
            "value": "devVM"
        }
    }
}